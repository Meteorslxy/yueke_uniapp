<wxs module="wxs">
function calContent(content) {
	var imgCnt = 0;
	var textCnt = 0;
	for (var k = 0; k < content.length; k++) {
		if (content[k].type == 'img') imgCnt++;
		if (content[k].type == 'text') textCnt++;
	}
	if (imgCnt || textCnt) {
		return textCnt + '段文字，' + imgCnt + '张图片';
	}
	else
		return '未填写';
}

module.exports = {
	calContent: calContent,
};

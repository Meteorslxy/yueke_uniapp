<view><view class="time-table"><view class="table-inner"><view class="list"><block wx:for="{{times}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item" data-idx="{{index}}" data-event-opts="{{[['tap',[['bindSelectTap',['$event']]]]]}}" bindtap="__e"><view class="{{['left '+(item.title?'top':'')]}}">{{item.title}}</view><view class="{{['right '+(item.selected?'selected':'')+'  '+(item.start==selectedStart?'selected-first':'')+' '+(item.expire?'expire':'')+' '+(item.used?'used':'')+'  '+(item.usedFirst?'used-first':'')+' text-cut']}}">{{''+item.usedText+''}}</view></view></block></view></view></view><view style="height:150rpx;"></view><block wx:if="{{selectedStart&&selectedEnd&&selectedEndPoint}}"><view data-event-opts="{{[['tap',[['bindSumbitTap',['$event']]]]]}}" class="time-submit safe-bottom" bindtap="__e"><view class="inner bg-project">{{"确认预约 ("+selectedStart+"~"+selectedEndPoint+")"}}</view></view></block></view>
<view><block wx:if="{{compName==='adminFormsDetailTpl'}}"><block wx:if="{{$root.g0>0}}"><view class="admin-comm-list"><view class="item"><view class="info"><block wx:for="{{forms}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.type!='line'}}"><view class="info-item info-item-solid"><view class="title">{{item.title}}</view><view class="mao">：</view><block wx:if="{{item.type=='image'}}"><view class="content"><block wx:for="{{item.val}}" wx:for-item="itm" wx:for-index="index1" wx:key="index1"><image class="loading" data-type="image" data-url="{{itm}}" src="{{itm}}" mode="aspectFill" show-menu-by-longpress="{{true}}" data-event-opts="{{[['tap',[['url',['$event']]]]]}}" bindtap="__e"></image></block></view></block><block wx:else><block wx:if="{{item.type=='rows'}}"><view class="content" style="flex-direction:column;"><block wx:for="{{item.val}}" wx:for-item="itm" wx:for-index="index1" wx:key="index1"><text><text>{{itm.title}}</text><block wx:if="{{itm.val}}"><text>{{"- "+itm.val}}</text></block></text></block></view></block><block wx:else><block wx:if="{{item.type=='switch'}}"><view class="content">{{item.val===true?'是':'否'}}</view></block><block wx:else><view class="content" data-type="copy" data-url="{{item.val}}" data-event-opts="{{[['tap',[['url',['$event']]]]]}}" bindtap="__e">{{item.val}}</view></block></block></block></view></block></block></view></view></view></block></block></view>
    display: flex;
    flex-wrap: wrap;
    padding: 25rpx;
    background-color: #fff;
    border-top: 4rpx solid #f2f2f2;
}
.time .item {
    width: calc((100% - 40rpx) / 3);
    background-color: #f2f2f2;
    border: 2rpx solid #f2f2f2;
    display: flex;
    flex-direction: column;
    align-items: center;

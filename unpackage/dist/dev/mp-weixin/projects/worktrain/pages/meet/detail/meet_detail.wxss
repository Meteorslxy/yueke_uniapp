.time .item .t2,
.time .item .t3 {
    margin-top: 5rpx;
}
.time .item .t2 {
    font-weight: bold;
}
.time .item .t3 {
    color: #666;
    font-size: 24rpx;
}
.time .item.cur .t3 {
    color: #000;

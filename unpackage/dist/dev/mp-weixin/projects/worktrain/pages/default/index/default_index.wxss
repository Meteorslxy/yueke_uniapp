.tabs .tab-menu.cur:after {
    border-radius: unset !important;
    background-color: unset !important;
    -webkit-transform: unset !important;
            transform: unset !important;
    border-top-left-radius: unset !important;
    border-top-right-radius: unset !important;
    border-bottom-right-radius: unset !important;
    border-bottom-left-radius: unset !important;

    content: '' !important;
    width: 30rpx !important;
    height: 30rpx !important;
    position: absolute !important;
    left: calc(50% - 15rpx) !important;
    bottom: 0rpx !important;
    border-top: 6rpx solid var(--projectCompareColor) !important;
    border-right: 6rpx solid var(--projectCompareColor) !important;
    border-top-right-radius: 100% !important;
    -webkit-transform: rotate(-225deg) !important;
            transform: rotate(-225deg) !important;
    border-top-width: 6rpx !important;
    border-top-style: solid !important;
    border-top-color: var(--projectCompareColor) !important;
    border-right-width: 6rpx !important;
    border-right-style: solid !important;
    border-right-color: var(--projectCompareColor) !important;
}
